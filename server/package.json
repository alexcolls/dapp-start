{
  "name": "portal-server",
  "version": "1.0.0",
  "description": "G4AL Portal - Marketplace - DAO",
  "_moduleAliases": {
    "@": "./src"
  },
  "main": "dist/index.js",
  "ignore": [
    "**/*.test.ts",
    "**/*.spec.ts",
    ".git",
    "node_modules"
  ],
  "watch": [
    "src"
  ],
  "ext": "ts, js",
  "scripts": {
    "start": "yarn && ts-node .",
    "build": "yarn && tsc -p .",
    "serve": "yarn build && firebase emulators:start --only functions",
    "shell": "yarn build && firebase functions:shell",
    "babel": "yarn && babel-node ./src/index.ts && ts-node-dev --respawn --transpile-only ./src/index.ts",
    "dev": "yarn && nodemon -r ts-node/register src/index.ts --verbose --watch",
    "test": "jest",
    "dist": "node dist/",
    "deploy": "yarn build && git add dist && git commit -m 'chore: deploy' && git push dist -b production"
  },
  "license": "ISC",
  "authors": "Alex Colls, Cristian Richarte, Carlos Falguera",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamesforaliving/portal-server"
  },
  "bugs": {
    "url": "https://github.com/gamesforaliving/portal-server/issues"
  },
  "homepage": "https://github.com/gamesforaliving/portal-server#readme",
  "keywords": [
    "G4AL Server",
    "G4AL Portal",
    "G4AL Marketplace",
    "G4AL DAO",
    "G4AL Social"
  ],
  "dependencies": {
    "@web3auth/node-sdk": "^1.1.0",
    "api": "^5.0.8",
    "assert": "^2.0.0",
    "axios": "^1.3.5",
    "child_process": "^1.0.2",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^2.5.2",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "ethers": "5.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase": "^9.22.0",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.4.0",
    "geocoder": "^0.2.3",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21",
    "lodash.isequal": "^4.5.0",
    "module-alias": "^2.2.2",
    "moment": "^2.17.1",
    "moment-duration-format": "^1.3.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.9",
    "openai": "^3.2.1",
    "pg": "^8.10.0",
    "promise": "^7.1.1",
    "regex": "^0.1.1",
    "request": "^2.88.2",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.0",
    "web3": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.2.3",
    "@types/module-alias": "^2.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.16.14",
    "@types/node-fetch": "^2.6.3",
    "@types/pg": "^8.6.6",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-transformer-keys": "^0.4.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.2",
    "tst-reflect": "^0.12.0",
    "typescript": "^4.6.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.10"
  },
  "engines": {
    "node": "18"
  },
  "private": true
}
